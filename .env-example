# Copy this file to .env and replace the placeholders with your real values.
# The app uses python-dotenv to automatically load these values when you run it.

# Replace with your app token and bot token
SLACK_BOT_TOKEN=<your-bot-token>
SLACK_APP_TOKEN=<your-app-token>

# Gemini / OpenAI-compatible API settings
GEMINI_API_KEY=<your-gemini-api-key>
GEMINI_API_BASE_URL=https://generativelanguage.googleapis.com/v1beta/openai/
GEMINI_MODEL=<your-prefered-gemini-model> # e.g. gemini-2.5-flash
